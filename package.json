{
  "name": "nc-devtest",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server/index.js",
    "build:client": "",
    "build:server": "tsc -p server",
    "build": "npm run build:client && npm run build:server",
    "test": "ng test --sourcemaps=false",
    "lint:client": "",
    "lint:server": "tslint --type-check --project \"./server/tsconfig.json\" \"server/**/*.ts\"",
    "lint": "npm run lint:client && npm run lint:server",
    "dev:client": "",
    "dev:server": "npm run build:server && concurrently \"tsc -w -p server\" \"nodemon dist/server/index.js\"",
    "start:prod": "npm run build && node dist/server/index.js",
    "webpack": "./node_modules/.bin/webpack"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "core-js": "^2.5.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "mysql2": "^1.4.1",
    "pdfkit": "^0.8.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.0",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.36",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "~8.0.22",
    "@types/pdfkit": "^0.7.35",
    "@types/winston": "^2.3.4",
    "concurrently": "^3.5.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~2.7.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.6.0",
    "typescript": "~2.4.2",
    "webpack": "^3.5.4"
  }
